{
  "name": "nutrient-dws-azure-word",
  "version": "1.0.0",
  "description": "MS Word Add-in for Nutrient.io DWS APIs with Azure Functions backend",
  "main": "index.js",
  "scripts": {
    "dev": "webpack serve --mode development",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "start": "npm run dev",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "validate": "office-addin-manifest validate manifest.xml",
    "sideload": "office-addin-debugging start manifest.xml",
    "dev-server": "office-addin-debugging start manifest.xml --dev-server",
    "azure:start": "func start",
    "azure:deploy": "func azure functionapp publish my-nutrient-proxy"
  },
  "keywords": [
    "office-addin",
    "word",
    "pdf",
    "nutrient",
    "azure-functions"
  ],
  "author": "Nutrient.io",
  "license": "MIT",
  "dependencies": {
    "@microsoft/office-js": "^1.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "office-ui-fabric-react": "^7.199.1",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.0.0",
    "office-addin-debugging": "^5.0.0",
    "office-addin-manifest": "^1.12.0",
    "ts-jest": "^29.0.0",
    "ts-loader": "^9.4.0",
    "typescript": "^5.0.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0",
    "webpack-dev-server": "^4.15.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
} 